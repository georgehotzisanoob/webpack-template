{
  "name": "webpack-app",
  "version": "1.0.0",
  "description": "webpack template for projects",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node -r esm \"./node_modules/.bin/webpack\" --config=\"webpack/config/webpack.prod.js\"",
    "start": "cross-env NODE_ENV=development node -r esm \"./node_modules/.bin/webpack\" --config=\"webpack/config/webpack.dev.js\"",
    "serve": "serve ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgehotzisanoob/webpack-template.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Tyler Durden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgehotzisanoob/webpack-template/issues"
  },
  "homepage": "https://github.com/georgehotzisanoob/webpack-template#readme",
  "devDependencies": {
    "app-root-path": "^3.0.0",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.2.0",
    "serve": "^11.3.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  }
}
